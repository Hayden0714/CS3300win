<!DOCTYPE html>
<html>
  <head>
    <title>CS3300win</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <% if user_signed_in? %>
    <%= render 'home/navbarsignedin' %>
  <% else %>
    <%= render 'home/navbarsignedout' %>
  <% end %>

  <%= render 'home/homescreen' %>
  
  <body>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
